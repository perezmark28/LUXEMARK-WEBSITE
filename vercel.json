{
  "version": 2,
  "builds": [
    {
      "src": "LuxeMark_Watches/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "buildCommand": "cd LuxeMark_Watches && npm install && npm run build",
  "outputDirectory": "LuxeMark_Watches/build",
  "routes": [
    { "src": "/(.*)", "dest": "/LuxeMark_Watches/build/$1" }
  ]
}
