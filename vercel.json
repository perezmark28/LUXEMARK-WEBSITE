{
  "builds": [
    {
      "src": "luxemark-watches/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "luxemark-watches/build" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "luxemark-watches/build/$1" },
    { "src": "/.*", "dest": "luxemark-watches/build/index.html" }
  ]
}
